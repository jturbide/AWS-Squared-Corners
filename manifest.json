{
  "manifest_version": 3,
  "name": "AWS Squared Corners",
  "version": "1.0",
  "description": "Restores squared corners for buttons in AWS.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://*.console.aws.amazon.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.console.aws.amazon.com/*"],
      "css": ["style.css"]
    }
  ]
}
